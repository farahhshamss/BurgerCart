<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.prj.burgercart.OrdersCart"></activity>
        <activity android:name="com.prj.burgercart.OrderItems" />
        <activity android:name="com.prj.burgercart.Menu" />
        <activity android:name="com.prj.burgercart.EditUser" />
        <activity android:name="com.prj.burgercart.EditUserProfile" />
        <activity android:name="com.prj.burgercart.ViewOrders" />
        <activity android:name="com.prj.burgercart.UserPanel" />
        <activity android:name="com.prj.burgercart.AdminPanel" />
        <activity android:name="com.prj.burgercart.Register" />
        <activity android:name="com.prj.burgercart.PendingOrders" />
        <activity android:name="com.prj.burgercart.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>